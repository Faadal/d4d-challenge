<?xml version="1.0" encoding="utf-8"?>
<Map srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" background-color="rgb(70,130,180)">
    <Style name="My Style">
        <Rule>
            <PolygonSymbolizer fill="rgb(242,239,249)"/>
            <LineSymbolizer stroke="rgb(128,128,128)" stroke-width="0.1000000014901161"/>
            <PointSymbolizer file=""/>
        </Rule>
    </Style>
    <Layer name="world" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
        <StyleName>My Style</StyleName>
        <Datasource>
            <Parameter name="file">resources/ne_110m_admin_0_countries.shp</Parameter>
            <Parameter name="type">shape</Parameter>
        </Datasource>
    </Layer>
    <Layer name="points" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
        <StyleName>My Style</StyleName>
        <Datasource>
            <Parameter name="file">resources/antennas.shp</Parameter>
            <Parameter name="type">shape</Parameter>
        </Datasource>
    </Layer>
</Map>
